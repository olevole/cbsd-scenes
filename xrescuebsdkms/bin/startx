#!/bin/sh
#v9.0.0
globalconf="${workdir}/cbsd.conf";
MYOPTARG="user"
MYDESC="X Configuration tools"

[ -f ${globalconf} ] || err 1 "no such conf file";
. ${globalconf}
. ${subr}
. ${inventory}
init $*


myconf()
{
[ $baserw -eq 1 ] && path=$data

get_jid
jid=$?
[ $jid -eq 0 ] && err 1 "Not running. Please jstart first"

chroot ${path} xinit

}
